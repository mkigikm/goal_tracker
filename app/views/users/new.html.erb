<% title "Welcome to Goal Tracker" %>

<h1>Sign up for Goal Tracker</h1>

<form action="<%= users_url %>" method="post">
  <%= auth_token %>

  <label for="user_name">Name</label>
  <input id="user_name" type="text" name="user[name]" value="">

  <label for="user_password">Password</label>
  <input id="user_password" type="password" name="user[password]" value="">

  <button>Sign Up</button>

</form>
